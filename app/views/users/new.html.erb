<h1>Sign up</h1>


<%= form_for(@user) do |f| %>
 <%= render 'shared/error_messages' %>
  <div class="field" >
    <%= f.label :username %>
    <%= f.text_field :name %>
  </div>

  <div class="field" >
    <%= f.label :email %>
    <%= f.text_field :email%>
  </div>
  
    

  <div class="actions" >
    <%= f.submit "Sign up" %>
  </div>
<% end %>
