<!DOCTYPE html>
<html>
	<head>
		<title><%= title %></title>
        <%= render 'layouts/stylesheets' %>
		<%= javascript_include_tag "application" %>
		<%= csrf_meta_tags %>
	</head>
	<body>
		<div class="container">
			 <% flash.each do |key, value| %>
     <div class="flash <%= key %>">
         <%= value %>
     </div>
  <% end %>
            <%= render 'layouts/header' %>
					<section class="round">
			 <%= render 'layouts/main' %>
						<%= yield%>
					</section>
			<%= render 'layouts/footer' %>
		</div>
		
		
	</body>
</html>
